export ZSH="$HOME/.oh-my-zsh"
export PATH="/home/jaintp/.local/bin:$PATH"

ZSH_THEME="jaintp"

plugins=(git)

source $ZSH/oh-my-zsh.sh

# User configuration
export TERM=xterm-256color

# Aliases
alias btw="fastfetch"
alias ..="cd .."
alias ls="lsd"
alias cursor='env XDG_DATA_DIRS=/usr/share:/usr/local/share cursor'
eval $(thefuck --alias)

